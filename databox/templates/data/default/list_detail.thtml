{# begin {templatelocation} #}
{# plugins/databox/templates/data/default/list_detail.thtml 20151109 #}
<div class="databox">
  <h2>{title}</h2>
  <p>{lang_id}: {id}</p>
  <p><span style="color:green"> &#123;languageid&#125;</span>:{languageid}
     <span style="color:green"> &#123;_languageid&#125;</span>:{_languageid}
     <span style="color:green"> &#123;language&#125;</span>:{language}
  </p>
  <p><span style="color:green"> &#123;uid&#125;</span>:{uid}
     <span style="color:green"> &#123;username&#125;</span>:{username}
     <span style="color:green"> &#123;fullname&#125;</span>:{fullname}
     <span style="color:green"> &#123;displayname&#125;</span>:{displayname}
  </p>
  <p><span style="color:green"> &#123;lang_download&#125;</span>:{lang_download}
     <span style="color:green"> &#123;lang_downloadrequired&#125;</span>:{lang_downloadrequired}
     <span style="color:green"> &#123;lang_display&#125;</span>:{lang_display}
     <span style="color:green"> &#123;lang_displayrequired&#125;</span>:{lang_displayrequired}
  </p>
{# Basic Items #}
  <fieldset style="width:98%">
    <legend>{lang_basicfields}</legend>
    <table style="width:98%" class="databox">
      <tr>
        <th style="width:20%;" class="alignright">{lang_field}</th>
        <th style="width:20%; color:green;">{lang_templatesetvar}</th>
        <th style="width:60%;">{lang_content}</th>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_code}</th>
        <td style="width:20%; color:green">&#123;code&#125;</td>
        <td>{code}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_title}</th>
        <td style="width:20%; color:green">&#123;title&#125;</td>
        <td>{title}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_description}</th>
        <td style="width:20%; color:green">&#123;description&#125;</td>
        <td>{description}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_hits}</th>
        <td style="width:20%; color:green">&#123;hits&#125;</td>
        <td>{hits}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_modified}</th>
        <td style="width:20%; color:green">&#123;modified&#125;</td>
        <td>{modified}&nbsp;{modified_shortdate}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_created}</th>
        <td style="width:20%; color:green">&#123;created&#125;&nbsp;&#123;created_shortdate&#125;</td>
        <td>{created}&nbsp;{created_shortdate}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_released}</th>
        <td style="width:20%; color:green">&#123;released&#125;&nbsp;&#123;released_shortdate&#125;</td>
        <td>{released}&nbsp;{released_shortdate}</td>
		</tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_expired}</th>
		<td style="width:20%; color:green">&#123;expired&#125;&nbsp;&#123;expired_shortdate&#125;</td>
        <td>{expired}&nbsp;{expired_shortdate}</td>
		</tr>

      <tr>
        <th style="width:20%" class="alignright">{lang_owner}</th>
        <td style="width:20%; color:green">&#123;owner_name&#125;</td>
        <td>{owner_name}<input type="hidden" name="owner_id" value="{owner_id}"{xhtml}>
        ( UID: <span  style="color:green"> &#123;owner_id&#125;</span>: {owner_id} )</td>
	　</tr>
	
      <tr>
        <th style="width:20%" class="alignright">{lang_udatetime}</th>
        <td style="width:20%; color:green">&#123;udatetime&#125;</td>
        <td>{udatetime}
	( UID: <span  style="color:green"> &#123;last_modify_uid&#125;</span>: {last_modify_uid}
name:<span  style="color:green"> &#123;last_modify_name&#125;</span>: {last_modify_name}	
fullname:<span  style="color:green"> &#123;last_modify_fullname&#125;</span>: {last_modify_fullname}	
		)</td>
	  </tr>

      <tr>
        <th style="width:20%" class="alignright">Maps Plugin mkid</th>
        <td style="width:20%; color:green">&#123;mkid&#125;&#123;marker&#125;</td>
        <td>{mkid} {marker}</td>
      </tr>
    </table>

    <table style="width:98%" class="databox">
		<tr>
        <th style="width:20%;" class="alignright">{lang_field} PATH</th>
        <th style="width:20%; color:green;">{lang_templatesetvar}</th>
        <th style="width:60%;">{lang_content}</th>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_imgfile_frd} URL</th>
        <td style="width:20%; color:green">&#123;imgfile_frd&#125;</td>
        <td>{imgfile_frd}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_imgfile_frd} PATH</th>
        <td style="width:20%; color:green">&#123;data_img_url&#125;</td>
        <td>{data_img_url}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_imgfile_thumb_frd} URL</th>
        <td style="width:20%; color:green">&#123;imgfile_thumb_frd&#125;</td>
        <td>{imgfile_thumb_frd}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_imgfile_thumb_frd}</th>
        <td style="width:20%; color:green">&#123;data_thumb_img_url&#125;</td>
        <td>{data_thumb_img_url}</td>
      </tr>
    </table>
  </fieldset>

{# Category #}
  <fieldset style="width:98%">
    <legend>{lang_category} </legend>
    <table style="width:98%" class="databox">
		<tr>
        <th style="width:20%;" class="alignright">{lang_field}</th>
        <th style="width:20%; color:green;">{lang_templatesetvar}</th>
        <th style="width:60%;">{lang_content}</th>
      </tr>
