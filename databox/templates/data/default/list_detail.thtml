		<td style="width:20%; color:green">
		&#123;categorieslinkid&#125;
		</td>
		<td>{categorieslinkid}</td>
	  </tr>
	

    </table>
  </fieldset>

{# Additional Items #}
  <fieldset style="width:98%">
    <legend>{lang_additionfields} </legend>
    <table style="width:98%" class="databox">
      <tr>
	<th style="width:20%;" class="alignright">{lang_field}<br{XHTML}>&#123;additionfield_name&#125;</th>
        <th style="width:40%; text-align:left" class="alignleft">
	  <dl class="databox_col_detail">
	    <dt style=color:green;>{lang_templatesetvar} &#123;additionfield_templatesetvar&#125;</dt>
	    <dd>{lang_content} &#123;additionfield_value&#125;</dd>
	  </dl>
	</th>
        <th class="alignleft">{lang_type}</th>
      </tr>
       {additionfields}
    </table>
  </fieldset>

