      <tr>
        <th style="width:20%" class="alignright">{lang_category}</th>
		<td style="width:20%; color:green">&#123;categories&#125;<br{xhtml}></td>
        <td>{categories}</td>
      </tr>
	  {# カテゴリ名称列挙リンク付 #}
      <tr>
        <th style="width:20%" class="alignright">
		{lang_category} {lang_withlink}
		</th>
		<td style="width:20%; color:green">
		&#123;categorieslink&#125;
		</td>
		<td>{categorieslink} </td>
        </tr>
	  <tr>
		<th style="width:20%" class="alignright">CODE形式</th>
		<td style="width:20%; color:green">
		&#123;categorieslinkcode&#125
		</td>
		<td>{categorieslinkcode}</td>
	  </tr>
	  <tr>
