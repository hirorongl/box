        <th style="width:20%" class="alignright">{lang_imgfile_frd}</th>
        <td style="width:20%; color:green" class="alignleft">&#123;imgfile_frd&#125;</td>
        <td >{imgfile_frd}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_imgfile_thumb_frd}</th>
        <td style="width:20%; color:green" class="alignleft">&#123;imgfile_thumb_frd&#125;</td>
        <td >{imgfile_thumb_frd}</td>
      </tr>
	
	  <tr>
        <th style="width:20%; background:#eee" class="alignright">{lang_field}</th>
        <td style="width:20%; color:green; background:#eee" class="alignleft">{lang_templatesetvar}</td>
        <td class="alignleft" style="background:#eee">{lang_content}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_description}</th>
        <td style="width:20%; color:green" class="alignleft">&#123;description&#125;</td>
        <td >{description}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_hits}</th>
        <td style="width:20%; color:green" class="alignleft">&#123;hits&#125;</td>
        <td >{hits}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_modified}</th>
        <td style="width:20%; color:green" class="alignleft">&#123;modified&#125;</td>
        <td >{modified}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_created}</th>
        <td style="width:20%; color:green" class="alignleft">&#123;created&#125;</td>
        <td >{created}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_category}</th>
        <td style="width:20%; color:green" class="alignleft">&#123;categories&#125;</td>
        <td >{categories}</td>
      </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_category} {lang_withlink}</th>
        <td style="width:20%; color:green" class="alignleft">&#123;categorieslink&#125;</td>
        <td >{categorieslink}</td>
	  </tr>
	
	
	<!-- @@@@@@ -->
      <tr>
        <th style="width:20%" class="alignright">{lang_udatetime}</th>
        <td style="width:10%; color:green" class="alignleft">&#123;udatetime&#125;</td>
        <td>{udatetime}
	( UID: <span  style="color:green"> &#123;last_modify_uid&#125;</span>: {last_modify_uid}
name:<span  style="color:green"> &#123;last_modify_name&#125;</span>: {last_modify_name}	
fullname:<span  style="color:green"> &#123;last_modify_fullname&#125;</span>: {last_modify_fullname}	
		)</td>
	 </tr>
      <tr>
        <th style="width:20%" class="alignright">{lang_draft}</th>
        <td style="width:20%; color:green" class="alignleft">&#123;draft&#125;</td>
        <td >{draft}</td>
      </tr>

    </table>
  </fieldset>

<!-- 追加項目 -->
  <fieldset style="width:94%">
    <legend>{lang_additionfields} {lang_byusingtemplatesetvar}</legend>
    <table style="width:98%">
      <tr>
        <th style="width:20%; background:#eee" class="alignright">{lang_field}</th>
        <td style="width:20%; color:green; background:#eee" class="alignleft">{lang_templatesetvar}</td>
        <td class="alignleft" style="background:#eee">{lang_content}</td>
      </tr>
      <tr>
        <th style="width:20%; color:green; background:#eee" class="alignright">&#123;additionfields&#125;  : &#123;additionfield_name&#125;</th>
        <td style="width:20%; color:green; background:#eee" class="alignleft">&#123;additionfield_templatesetvar&#125;</td>
        <td class="alignleft" style="color:green; background:#eee">&#123;additionfield_value&#125;</td>
      </tr>
    {additionfields}
    </table>
  </fieldset>
{draft} {edit}

