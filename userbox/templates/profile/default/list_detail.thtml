<div class="userbox">
  <h2>{title}</h2>
  <p>{lang_id}: {id}</p>
  <p><span style="color:green"> &#123;languageid&#125;</span>:{languageid}
     <span style="color:green"> &#123;_languageid&#125;</span>:{_languageid}
     <span style="color:green"> &#123;language&#125;</span>:{language}
  </p>

  <fieldset style="width:94%">
    <legend>Core</legend>
    <table style="width:98%">
      <tr>
        <th style="width:20%; background:#eee" class="alignright">{lang_field}</th>
        <td style="width:20%; color:green; background:#eee" class="alignleft">{lang_templatesetvar}</td>
        <td class="alignleft" style="background:#eee">{lang_content}</td>
      </tr>

        <!-- uid -->
        <tr>
            <th style="width:10%" class="alignright">{lang_uid}{help_uid}</th>
            <td style="width:20%; color:green; background:#eee" class="alignleft">uid</td>
            <td >
                {id}

            </td>
        </tr>

        <!-- username -->
        <tr>
            <th style="width:10%" class="alignright">{lang_username}{help_username}</th>
            <td style="width:20%; color:green; background:#eee" class="alignleft">username</td>
            <td >
                {username}

            </td>
        </tr>
        <!-- fullname -->
        <tr>
            <th style="width:10%" class="alignright">{lang_fullname}{help_fullname}</th>
            <td style="width:20%; color:green; background:#eee" class="alignleft">fullname</td>
            <td >
                {fullname}
            </td>
        </tr>

    </table>
  </fieldset>
<!--  基本項目 -->
  <fieldset style="width:94%">
    <legend>{lang_basicfields}</legend>
    <table style="width:98%">
	
	  <tr>
        <th style="width:20%; background:#eee" class="alignright">{lang_field}</th>
